{
    "almond": {},
    "bootstrap#3.x":{
        "shim":[
            "less/bootstrap.less",
            "dist/css/bootstrap.css",
            "dist/js/bootstrap.js",
            "dist/fonts/glyphicons-halflings-regular.eot",
            "dist/fonts/glyphicons-halflings-regular.svg",
            "dist/fonts/glyphicons-halflings-regular.ttf",
            "dist/fonts/glyphicons-halflings-regular.woff"
        ],
        "map": {
            "fonts":{
                "dist/fonts/": "/"
            },
            "less":{
                "flexslider.css": "flexslider.less"
            },
            "sass":{
                "flexslider.css": "_flexslider.scss"
            }
        },

    },

    "flexslider": {
        "shim":[
            "flexslider.css",
            "jquery.flexslider.js",
            "fonts/*"
        ],
        "map": {
            "fonts":{
                "fonts/": "/"
            },
            "less":{
                "flexslider.css": "flexslider.less"
            },
            "sass":{
                "flexslider.css": "_flexslider.scss"
            }
        },
        "replace": {
            "less":{
                "url\\('fonts/": "url('/assets/fonts/vendor/flexslider/"
            },
            "sass":{
                "url\\('fonts/": "url('/assets/fonts/vendor/flexslider/"
            }
        }
    },
    "handlebars": {
        "shim": [
            "handlebars.js",
            "handlebars.amd.js",
            "handlebars.runtime.js",
            "handlebars.runtime.amd.js"
        ],
        "map":{
            "scripts":{
                "":"/"
            }
        }
    },
    "html5shiv":{},
    "jquery#2.x":{},
    "jquery#1.x":{},
    "leaflet#0.x":{
        "shim":[
            "dist/leaflet.css",
            "dist/leaflet.js",
            "dist/images/*"
        ],
        "map": {
            "less":{
                "dist/leaflet.css": "leaflet.less"
            },
            "sass": {
                "dist/leaflet.css": "_leaflet.scss"
            }
        },
        "replace":{
            "less": {
                "url\\(images/": "url(/assets/images/vendor/leaflet/"
            },
            "sass": {
                "url\\(images/": "url(/assets/images/vendor/leaflet/"
            }
        }
    },
    "normalize-css": {
        "map":{
            "less":{
                "normalize.css": "normalize.less"
            },
            "sass":{
                "normalize.css": "_normalize.scss"
            }
        }
    },
    "q#1.x": {
        "shim": "q.js"
    },
    "requirejs":{},
    "requirejs-plugins": {
        "map":{
            "scripts": {
                "src": "/requirejs-plugins/",
                "lib": "/requirejs-plugins/"
            }
        }
    },
    "requirejs-text": {
        "shim": "text.js",
        "map":{
            "scripts":{
                "": "/requirejs-plugins/"
            }
        }
    }


}

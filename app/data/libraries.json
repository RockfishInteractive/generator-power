{
    "almond": {},
    "flexslider": {
        "shim":[
            "jquery.flexslider.js",
            "flexslider.less",
            "fonts/flexslider-icon.eot",
            "fonts/flexslider-icon.woff",
            "fonts/flexslider-icon.ttf",
            "fonts/flexslider-icon.svg"
        ],
        "map": {
            "fonts":{
                "flexslider/fonts/": "/"
            }
        }
    },
    "handlebars": {
        "shim": [
            "handlebars.js",
            "handlebars.amd.js",
            "handlebars.runtime.js",
            "handlebars.runtime.amd.js"
        ],
        "map":{
            "scripts":{
                "handlebars":"/"
            }
        }
    },
    "jquery#2.x":{},
    "jquery#1.x":{},
    "normalize-css": {
        "map":{
            "less":{
                "normalize-css/normalize.css": "normalize.less"
            },
            "sass":{
                "normalize-css/normalize.css": "_normalize.scss"
            }
        }
    },
    "q#1.x": {
        "shim": "q.js"
    },
    "requirejs":{},
    "requirejs-plugins": {
        "map":{
            "scripts": {
                "requirejs-plugins/src": "/requirejs-plugins/",
                "requirejs-plugins/lib": "/requirejs-plugins/"
            }
        }
    },
    "requirejs-text": {
        "shim": "text.js",
        "map":{
            "scripts":{
                "requirejs-text": "/requirejs-plugins/"
            }
        }
    }

}

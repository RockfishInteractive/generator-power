{
    "order": -999,
    "settings": {
        "build": {
            "root": "public",
            "scripts": "public/assets/scripts",
            "css": "public/assets/css",
            "fonts": "public/assets/fonts",
            "images": "public/assets/images",
            "templates": "public/assets/templates"
        },

        "resources": {
            "root": "resources",
            "fonts": "resources/fonts",
            "images": "resources/images",
            "icons": "resources/icons",
            "sketch": "resources/sketch"
        },

        "source": {
            "root": "source",
            "scripts": "source/scripts",
            "less": "source/less",
            "sass": "source/sass"
        },

        "dependencies": {
            "path": "vendor",
            "shim": {},
            "map": {
                "less": {},
                "sass": {},
                "fonts": {},
                "scripts": {},
                "images": {}
            },
            "extensions": {
                "scripts": ["js"],
                "less": ["less", "css"],
                "sass": ["sass", "css"],
                "fonts": ["woff", "eot", "ttf", "svg"],
                "images": ["png", "jpg", "svg", "gif"]
            },
            "replace": {
                "scripts": {},
                "less": {},
                "sass": {},
                "images": {}
            }
        },

        "style": {
            "optimize": true,
            "language": "less",
            "images": "../images",
            "fonts": "../fonts",
            "files": []
        },

        "scripts": {
            "debug": false,
            "compress": true,
            "copy": [],
            "concat": [],
            "require": {},
            "dependencies": []
        },

        "images": {
            "types": "png,jpg,jpeg,gif,svg,ico"
        },

        "fonts": {
            "engine": "fontforge",
            "iconFontName": "IconFont",
            "families": {}
        }
    }
